<div class="container">
    <div class="row">
        <div class="col-2"></div>
        <div class="col-8">
            <% if (errors.length> 0 ){ %>
                <div class="alert alert-danger">
                    <% errors.forEach((error) => { %>
                        <p class="fw-bold mt-1"><%= error %></p>
                    <% })%>
                </div>
                <%} %>
            <% if (success.length> 0 ){ %>
                <div class="alert alert-succss">
                    <% success.forEach((succes) => { %>
                        <p class="fw-bold mt-1"><%= succes %></p>
                    <% })%>
                </div>
                <%} %>

        </div>
        <div class="col-2"></div>
    </div>
</div>